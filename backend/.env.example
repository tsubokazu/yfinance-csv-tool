# Application
ENVIRONMENT=development
PROJECT_NAME="yfinance Trading Platform"
VERSION=1.0.0

# Security
SECRET_KEY=your-secret-key-here-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Redis
REDIS_URL=redis://localhost:6379/0

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# Tachibana Securities API
TACHIBANA_USER_ID=your-tachibana-user-id
TACHIBANA_PASSWORD=your-tachibana-password
TACHIBANA_DEMO_MODE=true
TACHIBANA_API_VERSION=v4r7
TACHIBANA_DEMO_BASE_URL=https://demo-kabuka.e-shiten.jp/e_api_v4r7/
TACHIBANA_PROD_BASE_URL=https://kabuka.e-shiten.jp/e_api_v4r7/
TACHIBANA_SESSION_TIMEOUT=3600

# Sentry (Optional)
SENTRY_DSN=

# CORS
CORS_ORIGINS=["http://localhost:3000"]

# Database (if using direct connection)
DATABASE_URL=postgresql://user:password@localhost/dbname

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Cache Settings
CACHE_TTL_MINUTES=5
CACHE_TTL_HOURS=1
CACHE_TTL_DAYS=1